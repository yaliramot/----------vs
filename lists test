odd_squares = {x: x**2 for x in range(1,11) if x % 2 != 0}
print("Odd squares:", odd_squares)
multiples_of_3 = {x: x*3 for x in range(1,50) if x % 4 != 0}
print("Multiples of 3:", multiples_of_3)


number= [1, 2, 3, 4, 5, 6]

even_numbers = list(filter(lambda x: x % 2 == 0, number))
print(even_numbers)